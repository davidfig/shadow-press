{
    "applicationName": "shadow-press",
    "disqus": "",
    "openGraph": {
        "title": "",
        "type": "website",
        "image": "",
        "site_name": "",
        "url": ""
    },
    "meta": {
        "title": "Shadow Press",
        "description": "",
        "keyword": ""
    },
    "linenums": true,
    "source": {
        "include": [
            "./index.js"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "readme": "./README.md",
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true,
        "destination": "./docs/jsdoc",
        "template": "./node_modules/yy-jsdoc-template"
    },
    "plugins": [
        "./node_modules/yy-jsdoc-template/plugins/skip-node_modules",
        "plugins/markdown"
    ],
    "readmeShowNav": "ShadowPress",
    "templates": {
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "shadow-press",
        "footer"        : "by YOPEY YOPEY LLC (yopeyopey.com)",
        "copyright"     : "Copyright Â© 2018 YOPEY YOPEY LLC.",
        "meta": {
            "title": "shadow-press API Documentation",
            "description": "Documentation for shadow-press library",
            "keyword": "docs, documentation, html5, javascript, jsdoc, shadow, button, press"
        }
    }
}